class A{
	public static int a = 10;
	public static void doA(){
		System.out.println("static doA 입니다.");
	}
	
	private void doB(){
		System.out.println("private doB입니다.");
	}
	
	public void doC(){
		doB();
		System.out.println("public doC입니다.");
	}
}

public class Ex01 {
	public static void main(String[] args) {
		A.doA();
	}
}
