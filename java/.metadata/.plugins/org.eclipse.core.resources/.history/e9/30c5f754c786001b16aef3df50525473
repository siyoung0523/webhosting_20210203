package HW;

import java.sql.*;

import DB.BUYTBL;
import DB.DBConnection;
import oracle.jdbc.*;

public class CallProcedure_insert {

	public void callprocedure_insert()  {
		// TODO Auto-generated method stub
		Connection conn = null; // DB연결된 상태(세션)을 담은 객체
        PreparedStatement pstm = null;  // SQL 문을 나타내는 객체
        ResultSet rs = null;  // 쿼리문을 날린것에 대한 반환값을 담을 객체
		try {	
			int count =0;
			BUYTBL[] buytbl = new BUYTBL[50];
//			 Class.forName("oracle.jdbc.driver.OracleDriver");
//			 Connection con = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:orcl","hr","123456");
			conn = DBConnection.getConnection();
			 // 프로시저 호출
			 CallableStatement cs = conn.prepareCall("begin add_buytbl(?,?,?,?,?,?,?); end;");
			 // 입력 파라메터
			 cs.setInt(1, 30);
			 cs.setString(2, "SSK");
			 cs.setString(3, "운동화");
			 cs.setString(4, null);
			 cs.setInt(5, 100);
			 cs.setInt(6, 3);
			 cs.setString(7, "코멘트");
			 int seq= cs.executeUpdate();
//			 ResultSet rs = (ResultSet)cs.getObject(1);
			 
			 if ( seq ==0 )
			 {
				 System.out.println(seq+"값");
			 }
			 else
			 {
				 System.out.println(seq+"값");
			 }
			 
//			 rs.close();
			 cs.close();
			 conn.close();
			 } catch(Exception e){
			 e.printStackTrace();
			 }
	}
}
